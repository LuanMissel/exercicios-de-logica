algoritmo "PROCEDIMENTOsoma"
var
   X, Y : INTEIRO

procedimento Soma(A,B :inteiro)
inicio

             escreval ("recebi o valor de ", A)
             escreval ("recebi o valor de ", B)
             escrevaL ("a soma entre os dois é: ", A + B)
FimProcedimento
inicio
      X <- 7
      Y <- 9
      Soma (X,Y)
fimalgoritmo