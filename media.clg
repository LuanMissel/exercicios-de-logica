algoritmo semNome;
// Síntese
//  Objetivo:  
//  Entrada :
//  Saída   :


principal
	// Declarações
	inteiro aluno;
	real nota, soma, media;
	// Instruções
	soma = 0;
	aluno = 1;
  	 faca
			escreva("Digite a nota do ", aluno, " aluno: ");
			leia (nota);
			soma = soma + nota;
			aluno = aluno + 1;
   	enquanto (aluno <= 5 ) ;
	
	media = soma / 5;
	escreva ("A média da turma é: ", media:3:1);	
fimPrincipal
